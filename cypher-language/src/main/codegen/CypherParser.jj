List<CypherHint> Hints() :
{
    Token t;
    boolean seek;
    CypherVariable v;
    Token labelOrRelType;
    List<CypherVariable> joinVariables;
    CypherHint hint;
    List<CypherHint> hints = null;
}
{
    ( t=<USING>
        (
              <INDEX> hint = IndexHintBody( HintIndexType.ANY, pos( t ) )
            | <BTREE> <INDEX> hint = IndexHintBody( HintIndexType.BTREE, pos( t ) )
            | <TEXT> <INDEX> hint = IndexHintBody( HintIndexType.TEXT, pos( t ) )
            | <JOIN> <ON> joinVariables=VariableList1() { hint = new CypherJoinHint( pos( t ), joinVariables ); }
            | <SCAN> v=Variable() labelOrRelType=LabelOrRelType() { hint = new CypherScanHint( pos( t ), v, labelOrRelType.image ); }
        )
        {
            if ( hints == null )
            {
                hints = new ArrayList<>();
            }
            hints.add( hint );
        }
    )*
    {
        return hints;
    }
}

